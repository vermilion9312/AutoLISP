(defun c:qq ()
  (vl-load-com)
  (setq pline (car (entsel "\nSelect a rectangle: ")))
  (if (setq vertices (vlax-invoke-method pline 'Vertices))
    (if (= (vla-get-count vertices) 4) ; Check if it's a rectangle (4 vertices)
      (progn
        (setq point1 (vlax-get-property (vlax-indexed-property-get vertices 0) 'Coordinates))
        (setq point2 (vlax-get-property (vlax-indexed-property-get vertices 1) 'Coordinates))
        (setq point3 (vlax-get-property (vlax-indexed-property-get vertices 2) 'Coordinates))
        (setq point4 (vlax-get-property (vlax-indexed-property-get vertices 3) 'Coordinates))
        (princ (strcat "\nPoint 1: " (vl-string-right-trim "}" (vlax-safearray->list point1))))
        (princ (strcat "\nPoint 2: " (vl-string-right-trim "}" (vlax-safearray->list point2))))
        (princ (strcat "\nPoint 3: " (vl-string-right-trim "}" (vlax-safearray->list point3))))
        (princ (strcat "\nPoint 4: " (vl-string-right-trim "}" (vlax-safearray->list point4))))
      )
      (prompt "\nSelected entity is not a rectangle.")
    )
    (prompt "\nNo entity selected or the selected entity is not a polyline.")
  )
  (princ)
)
